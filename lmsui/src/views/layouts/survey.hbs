	<script type="text/javascript">


		function mytimeout() {
			swal({
				title: "Your session is about to timeout?",
				text: "Do you want to logout now?",
				icon: "warning",
				buttons: true,
				dangerMode: true,
			}).then((willDelete) => {
				if (willDelete) {
					window.location.replace("/logout");
				} else {
					// don't let it timeout.
					// restart the timer
					setTimeout(mytimeout, 900000);
					swal("Please complete this test ASAP!", {
						icon: "warning",
					});
				}
			});
		}
		setTimeout(mytimeout, 900000);
	</script>
